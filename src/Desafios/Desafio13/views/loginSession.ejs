</head>

<body>
    <div class="container">
        <header>
            <%- include('./partials/header', {current: `loginSession`}) %>
        </header>
        <main>

            <div class="jumbotron">
                <form method="GET" action=/api/productos-test>
                    <h2 class="fw-bold mb-2 text-uppercase">Login</h2>

                    <p>Usuario</p>
                    <div class="form-group">
                        <input type="text" id="typeEmailX" name="nameLogin" class="form-control form-control-lg"
